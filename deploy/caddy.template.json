{
  "domain": "{{YOUR_DOMAIN}}",
  "listen_port": 8000,
  "workers": "auto",
  "tls": {
    "email": "you@example.com",
    "provider": "lets-encrypt"
  },
  "reverse_proxy": [
    {
      "match": ["/ws"],
      "upstream": "127.0.0.1:8000",
      "websocket": true
    },
    {
      "match": ["/sse"],
      "upstream": "127.0.0.1:8000",
      "buffering": false
    },
    {
      "match": ["/"],
      "upstream": "127.0.0.1:8000"
    }
  ]
}
